no ip http secure-server
ip nat inside source static 10.200.200.22 50.50.50.22 vrf CSNI
ip nat inside source static 10.0.100.40 50.50.50.40 vrf CSNI
ip route vrf CSNI 50.50.50.0 255.255.255.0 Null0
!
ip access-list extended CN-MISSION-SRV-TO-US-MISSION-SRV-TRAFFIC
 remark Allow CSNI NAT Pool to Communicate with US NAT POOL
 permit ip 50.50.50.0 0.0.0.255 10.0.200.32 0.0.0.31
 permit ip 10.0.200.32 0.0.0.31 50.50.50.0 0.0.0.255
 remark Allow BGP between CSNI and MSCI Peer #1
 remark STIG NET0410
 permit tcp host 10.200.200.2 eq bgp host 10.200.200.1
 remark Allow BGP between CSNI and MSCI Peer #2
 remark STIG NET0410
 permit tcp host 10.200.200.6 eq bgp host 10.200.200.5
 remark STIG NET0892 Block SNMP on all external interfaces
 remark STIG NET0923 Block loopback address 127.0.0.0/8
 remark deny ip 127.0.0.0 0.255.255.255 any log
 remark STIG NET0924 Block local-link 169.254.0.0/16
 remark deny ip 169.254.0.0 0.0.255.255 any log
 remark STIG NET0911 - Inbound ICMP messages are blocked 
 remark STIG NET0918 - Deny ICMP Type 11
 remark Permit ICMP ECHO/REPLY
 permit icmp any any echo
 permit icmp any any echo-reply
 deny   icmp any any log
ip access-list extended US-MISSION-SRV-TO-CN-MISSION-SRV-TRAFFIC
 permit ip 50.50.50.0 0.0.0.255 10.0.200.32 0.0.0.31
!
!
ip prefix-list CMAF-CORE1-LOOPBACK seq 5 permit 10.200.200.35/32
!
ip prefix-list CMAF-CORE2-LOOPBACK seq 5 permit 10.200.200.36/32
!
ip prefix-list CSNI-SERVERS-NATPOOL seq 5 permit 10.0.200.32/27
!
ip prefix-list US-NATPOOL-ONLY seq 5 permit 50.50.50.0/24
logging trap notifications
!
route-map CSNI-ROUTES-INTO-OSPF permit 10
 match ip address prefix-list CSNI-SERVERS-NATPOOL
 match route-type external
!
route-map EGRESS-SECONDARY-PATH permit 10
 set as-path prepend 300 300
!
route-map ADVERTISE-NATPOOL-PRIMARY permit 10
 match ip address prefix-list US-NATPOOL-ONLY
 set as-path prepend 300
!
route-map ADVERTISE-NATPOOL-SECONDARY permit 10
 match ip address prefix-list US-NATPOOL-ONLY
 set as-path prepend 300 300
!
route-map EGRESS-PRIMARY-PATH permit 10
 set as-path prepend 300
!
route-map CORE-REACHABLE permit 10
 match ip address prefix-list CMAF-CORE1-LOOPBACK
!
route-map CORE-REACHABLE permit 20
 match ip address prefix-list CMAF-CORE2-LOOPBACK
!
!
!
