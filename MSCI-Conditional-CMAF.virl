 ip address 10.200.200.33 255.255.255.255
!
interface Loopback3
 ip vrf forwarding CSNI
 ip address 50.50.50.1 255.255.255.0
!
interface GigabitEthernet0/0
 description OOB Management
 ip address 10.255.0.182 255.255.0.0
 shutdown
 duplex auto
 speed auto
 media-type rj45
!
interface GigabitEthernet0/1
 description to CSNI-OR-01
 ip vrf forwarding CSNI
 ip address 10.200.200.1 255.255.255.252
 ip access-group CN-MISSION-SRV-TO-US-MISSION-SRV-TRAFFIC in
 ip access-group US-MISSION-SRV-TO-CN-MISSION-SRV-TRAFFIC out
