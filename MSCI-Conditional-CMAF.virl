 router-id 10.200.200.32
 capability vrf-lite
 area 0 authentication message-digest
 network 10.200.200.13 0.0.0.0 area 0
 network 10.200.200.17 0.0.0.0 area 0
 network 10.200.200.32 0.0.0.0 area 0
!
router ospf 1002 vrf CSNI
 router-id 10.200.200.33
 capability vrf-lite
 area 0 authentication message-digest
 redistribute bgp 300 metric 300 subnets route-map CSNI-ROUTES-INTO-OSPF
 network 10.200.200.9 0.0.0.0 area 0
 network 10.200.200.33 0.0.0.0 area 0
!
router bgp 300
 bgp router-id 10.200.200.33
 bgp log-neighbor-changes
 bgp default local-preference 500
 !
 address-family ipv4 vrf CSNI
  network 50.50.50.0 mask 255.255.255.0
  neighbor 10.200.200.2 remote-as 64000
  neighbor 10.200.200.2 description &amp;lt;== eBGP CSNI-01 ==&amp;gt;
  neighbor 10.200.200.2 password 7 021605481811003348
  neighbor 10.200.200.2 update-source GigabitEthernet0/1
  neighbor 10.200.200.2 timers 10 30
  neighbor 10.200.200.2 activate
  neighbor 10.200.200.2 advertise-map ADVERTISE-NATPOOL-PRIMARY exist-map CORE-REACHABLE
  neighbor 10.200.200.2 soft-reconfiguration inbound
  neighbor 10.200.200.2 prefix-list CSNI-SERVERS-NATPOOL in
  neighbor 10.200.200.2 prefix-list US-NATPOOL-ONLY out
  neighbor 10.200.200.2 route-map EGRESS-PRIMARY-PATH in
  neighbor 10.200.200.6 remote-as 64000
  neighbor 10.200.200.6 description &amp;lt;== eBGP CSNI-02 ==&amp;gt;
  neighbor 10.200.200.6 password 7 021605481811003348
  neighbor 10.200.200.6 update-source GigabitEthernet0/2
  neighbor 10.200.200.6 timers 10 30
  neighbor 10.200.200.6 activate
  neighbor 10.200.200.6 advertise-map ADVERTISE-NATPOOL-SECONDARY exist-map CORE-REACHABLE
  neighbor 10.200.200.6 soft-reconfiguration inbound
  neighbor 10.200.200.6 prefix-list CSNI-SERVERS-NATPOOL in
  neighbor 10.200.200.6 prefix-list US-NATPOOL-ONLY out
  neighbor 10.200.200.6 route-map EGRESS-SECONDARY-PATH in
  neighbor 10.200.200.35 remote-as 300
  neighbor 10.200.200.35 description &amp;lt;== iBGP CMAF-CORE-01 ==&amp;gt;  
  neighbor 10.200.200.35 password 7 021605481811003348
  neighbor 10.200.200.35 update-source Loopback2
  neighbor 10.200.200.35 timers 10 30
  neighbor 10.200.200.35 activate
  neighbor 10.200.200.35 next-hop-self
  neighbor 10.200.200.35 soft-reconfiguration inbound
  neighbor 10.200.200.35 prefix-list CSNI-SERVERS-NATPOOL out
  neighbor 10.200.200.36 remote-as 300
  neighbor 10.200.200.36 description &amp;lt;== iBGP CMAF-CORE-01 ==&amp;gt;  
  neighbor 10.200.200.36 password 7 021605481811003348
  neighbor 10.200.200.36 update-source Loopback2
  neighbor 10.200.200.36 timers 10 30
  neighbor 10.200.200.36 activate
  neighbor 10.200.200.36 next-hop-self
  neighbor 10.200.200.36 soft-reconfiguration inbound
  neighbor 10.200.200.36 prefix-list CSNI-SERVERS-NATPOOL out
