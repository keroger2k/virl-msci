 duplex auto
 speed auto
 media-type rj45
!
interface GigabitEthernet0/3.20
 description &lt;== Outer Router to Firewall - SIPR Side ==&gt;
 encapsulation dot1Q 20
 ip vrf forwarding SIPR
 ip address 10.200.200.13 255.255.255.252
 no ip redirects
 no ip unreachables
 no ip proxy-arp
 ip ospf message-digest-key 1 md5 password
!
interface GigabitEthernet0/3.21
 description &lt;== Outer Router to Firewall - CSNI Side ==&gt;
 encapsulation dot1Q 21
 ip vrf forwarding CSNI
 ip address 10.200.200.9 255.255.255.252
 no ip redirects
 no ip unreachables
 no ip proxy-arp
 ip nat inside
 ip virtual-reassembly in
 ip ospf message-digest-key 1 md5 password
!
interface GigabitEthernet0/4
 ip vrf forwarding SIPR
 ip address 10.200.200.17 255.255.255.252
 no ip redirects
 no ip unreachables
 no ip proxy-arp
 ip ospf message-digest-key 1 md5 password
 duplex auto
 speed auto
 media-type rj45
!
interface GigabitEthernet0/5
 ip vrf forwarding MGMT
 ip address 10.200.199.3 255.255.255.240
 duplex auto
 speed auto
 media-type rj45
!
